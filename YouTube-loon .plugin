#!name=𝐘𝐨𝐮𝐓𝐮𝐛𝐞🚫/⿻/▶︎ɢʀ版  
#!desc=Youtube & Youtube Music 去广告/画中画/后台播放；支持快捷搜索关键词跳转等功能  
#!author=deezertidal  
#!icon=https://upload.wikimedia.org/wikipedia/commons/4/42/YouTube_icon_%282013-2017%29.png  
#!tag=YouTube去广告  
#!system=iOS  
#!system_version=15.0  
#!loon_version=3.2.1(372)

#!arguments=blockUpload:false,blockImmersive:false,captionLang:zh-Hans,lyricLang:zh-Hans,engine:auto,debug:false  
#!arguments-desc=  
- 参数值格式说明：true/false（开关）；zh-Hans（翻译语言）  
- 脚本引擎：[auto, jsc, webview]，推荐 jsc  
- 设置 debug 为 true 可输出调试信息日志

[Argument]
blockUpload = select, true, false, default=false, tag=屏蔽上传按钮, desc=是否屏蔽 YouTube 上传按钮
blockImmersive = select, true, false, default=false, tag=屏蔽选段按钮, desc=是否屏蔽 YouTube 选段按钮
captionLang = input, zh-Hans, tag=字幕翻译语言, desc=设置字幕翻译的目标语言（填 off 可关闭）
lyricLang = input, zh-Hans, tag=歌词翻译语言, desc=设置歌词翻译的目标语言（填 off 可关闭）
engine = select, auto, jsc, webview, default=auto, tag=脚本执行引擎, desc=选择脚本执行引擎
debug = select, true, false, default=false, tag=启用调试模式, desc=是否输出调试日志

[Rule]
AND,((DOMAIN-SUFFIX,googlevideo.com),(PROTOCOL,UDP)),REJECT
AND,((DOMAIN-SUFFIX,youtubei.googleapis.com),(PROTOCOL,UDP)),REJECT

[URL Rewrite]
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad _ REJECT-IMG

[mock-response-body]
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad mock-response-body data-type=text status-code=502

[Script]
http-response ^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch) script-path=https://raw.githubusercontent.com/guren777/Shadowrocket/main/youtube.response.preview.js,requires-body=true,binary-body-mode=true,timeout=10,tag=YouTube_Response_Modification,argument={"blockUpload":"{blockUpload}","blockImmersive":"{blockImmersive}","captionLang":"{captionLang}","lyricLang":"{lyricLang}","debug":"{debug}"}

[MITM]
hostname = %APPEND% *.googlevideo.com, youtubei.googleapis.com